<?xml version="1.0" encoding="UTF-8"?>
<widget>
<info>
<PluginName>Facebook Page Timeline</PluginName>
<PluginVersion>1.0.0</PluginVersion>
<ApplicationVersion>2.3.1</ApplicationVersion>
</info>
<dict>
<item><key>Plugin Name</key><string>Facebook Page Timeline</string></item>
<item><key>DefaultWidth</key><integer>340</integer></item>
<item><key>DefaultHeight</key><integer>500</integer></item>

<item>	
	<key>MinHeight</key>
	<integer>70</integer>
</item>	
<item>	
	<key>UseServerPreview</key>
	<boolean>TRUE</boolean>
</item>
<item>	
	<key>MinWidth</key>
	<integer>180</integer>
</item>	

<item>
	<key>HeadCode</key>
	<string><![CDATA[
	
]]>
            </string>   
</item>


<item><key>HTMLCode</key><string>
<![CDATA[ 

<div id="fb-root"></div>
<script>(function(d, s, id) {
  var js, fjs = d.getElementsByTagName(s)[0];
  if (d.getElementById(id)) return;
  js = d.createElement(s); js.id = id;
  js.src = "https://connect.facebook.net/en_US/sdk.js#xfbml=1&version=v2.10&appId=167769036628395";
  fjs.parentNode.insertBefore(js, fjs);
}(document, 'script', 'facebook-jssdk'));</script>


<div class="fb-page" data-href="{!-plyfbtlFacebookURL-!}" data-tabs="{!-plyfbtlShowTimeline(TRUE)//timeline//plyfbtlShowTimeline-!}{!-plyfbtlShowEvents(TRUE)//,events//plyfbtlShowEvents-!}{!-plyfbtlShowMessages(TRUE)//,messages//plyfbtlShowMessages-!}" width="{!-$Width-!}" height="{!-$Height-!}" data-small-header="{!-plyfbtlUseSmallHeader-!}" data-adapt-container-width="true" data-hide-cover="{!-plyfbtlHideCover-!}" data-show-facepile="{!-plyfbtlShowFriends-!}"><blockquote cite="{!-plyfbtlFacebookURL-!}" class="fb-xfbml-parse-ignore"><a href="{!-plyfbtlFacebookURL-!}">Everweb</a></blockquote></div>
]]></string></item>

</dict>
<options>
<option>
<displayName>Webpage Address</displayName>
<optionName>plyfbtlFacebookURL</optionName>
<ControlWidth>250</ControlWidth>
<defaultValue>http://www.facebook.com/everwebapp</defaultValue>
<OnNewLine>TRUE</OnNewLine>
<optionDescription>Facebook URL</optionDescription>
<controlType>1</controlType>
<textFieldMaxLength>0</textFieldMaxLength>
<Value>**N/A**</Value>
<processingCode><![CDATA[
dim Value, name As String
Dim rowText As String
name=Input("dictVariableName")
value=Input("dictVariableValue")
rowText=Input("dictcode")
if value.instr("://") = 0 then
//add our http://
value = "http://" + value
end
print rowText.Replace("{!-"+name+"-!}",value)]]></processingCode></option>


<option><optionName>plyfbtlShowTimeline</optionName><displayName>Show Timeline</displayName><defaultValue>TRUE</defaultValue><optionDescription>Show timeline</optionDescription><controlType>4</controlType><ControlWidth>200</ControlWidth><value>**N/A**</value></option>

<option><optionName>plyfbtlShowEvents</optionName><displayName>Show Events</displayName><defaultValue>TRUE</defaultValue><optionDescription>Show events</optionDescription><controlType>4</controlType><ControlWidth>200</ControlWidth><value>**N/A**</value></option>

<option><optionName>plyfbtlShowMessages</optionName><displayName>Show Messages</displayName><defaultValue>TRUE</defaultValue><optionDescription>Show events</optionDescription><controlType>4</controlType><ControlWidth>200</ControlWidth><value>**N/A**</value></option>



<option><optionName>plyfbtlHideCover</optionName><displayName>Hide Cover Photo</displayName><defaultValue>FALSE</defaultValue><optionDescription>Hide cover photo</optionDescription><controlType>4</controlType><ControlWidth>200</ControlWidth><value>**N/A**</value></option>

<option><optionName>plyfbtlShowFriends</optionName><displayName>Show friends photos</displayName><defaultValue>TRUE</defaultValue><optionDescription>Show friends photos</optionDescription><controlType>4</controlType><ControlWidth>200</ControlWidth><value>**N/A**</value></option>

<option><optionName>plyfbtlUseSmallHeader</optionName><displayName>Use Smaller Header</displayName><defaultValue>TRUE</defaultValue><optionDescription>Use Smaller Header</optionDescription><controlType>4</controlType><ControlWidth>200</ControlWidth><value>**N/A**</value></option>

<windowOption>
  <help>
  <videoURL>
  http://www.youtube.com/embed/mRWFkR4Xfik?rel=0
  </videoURL>
  </help>
  
  </windowOption>



</options>


</widget>