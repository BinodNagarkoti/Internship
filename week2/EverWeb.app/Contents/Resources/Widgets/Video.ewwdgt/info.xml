<item>
	<key>HTMLCode</key>
	<string><![CDATA[<!--{!-$width-!}--!>
	<video id="video{!-SHAPEINDEX-!}" width="100%" height="{!-$Height-!}"{!-plvdAutoplay(TRUE)// autoplay//plvdAutoplay-!}{!-plvdLoop(TRUE)// loop//plvdLoop-!} poster="{!-plvdPosterImage-!}">
	
	<source type="video/mp4" src="{!-pltsMP4UUID-!}" />
    <!-- WebM/VP8 for Firefox4, Opera, and Chrome -->
    <source type="video/webm" src="{!-pltsWEBMUUID-!}" />
    <!-- Ogg/Vorbis for older Firefox and Opera versions -->
    <source type="video/ogg" src="{!-pltsOGGUUID-!}" />
	</video>	
	<script>
$('#video{!-SHAPEINDEX-!}').mediaelementplayer( {
    features: [{!-plvdControls(TRUE)//'playpause','progress','current','duration','tracks','volume','fullscreen'//plvdControls-!}],
    enableKeyboard: true,
    pauseOtherPlayers: true,
    keyActions: []
 
});

vol = {!-plvdVolume-!};

switch (vol)
{
case 0:
	$volume = '0.0';
	break;
case 10:
	$volume = '1.0';
	break;
default:
	$volume = '0.' + vol;
	break;
}

video = document.getElementById('video{!-SHAPEINDEX-!}');
video.setVolume($volume);
video.setMuted({!-plvdMute(FALSE)//false//plvdMute-!}{!-plvdMute(TRUE)//true//plvdMute-!});


</script>
	
	]]>
	</string>
</item>

